@model Capa_Presentacion.Models.PrestamoEquipo

@{
    ViewBag.Title = "Detalles";
    string Rol = System.Web.HttpContext.Current.Session["Rol"] as String;
    if (Rol == "Administrador")
    {
        Layout = "~/Views/Shared/_Administrador.cshtml";
    }
    else if (Rol == "Administrador Servicios Bibliotecarios")
    {
        Layout = "~/Views/Shared/_AdministradorSB.cshtml";
    }
    else if (Rol == "Editar Servicios Bibliotecarios")
    {
        Layout = "~/Views/Shared/_EditarSB.cshtml";
    }
    else if (Rol == "Consultar")
    {
        Layout = "~/Views/Shared/_Consultar.cshtml";
    }
    else
    {

    }
}


<div class="content">
    <div>
        <h4 class="principal-texto">DETALLES PRÉSTAMO DE EQUIPO</h4>
        <div class="box box-primary">
            <div class="col-lg-12">
                <div class="col-lg-6">
                    <h6 class="principal-texto">DATOS DEL SOLICITANTE</h6>
                    <div class="box box-danger">
                        <div class="col-lg-12">
                            @Html.LabelFor(model => model.NombreSolicitante)
                            @Html.EditorFor(model => model.NombreSolicitante, new { htmlAttributes = new { @class = "form-control", @disabled = "disabled" } })
                        </div>
                        <div class="col-lg-12">
                            @Html.LabelFor(model => model.ApellidoSolicitante1)
                            @Html.EditorFor(model => model.ApellidoSolicitante1, new { htmlAttributes = new { @class = "form-control", @disabled = "disabled" } })
                        </div>
                        <div class="col-lg-12">
                            @Html.LabelFor(model => model.ApellidoSolicitante2)
                            @Html.EditorFor(model => model.ApellidoSolicitante2, new { htmlAttributes = new { @class = "form-control", @disabled = "disabled" } })
                        </div>
                        <div class="col-lg-12">
                            @Html.LabelFor(model => model.CedulaSolicitante)
                            @Html.EditorFor(model => model.CedulaSolicitante, new { htmlAttributes = new { @class = "form-control", @disabled = "disabled" } })
                        </div>
                        <div class="col-lg-12">
                            @Html.LabelFor(model => model.GeneroSolicitante)
                            @Html.EditorFor(model => model.GeneroSolicitante, new { htmlAttributes = new { @class = "form-control", @disabled = "disabled" } })
                        </div>
                    </div>
                </div>
                <div class="col-lg-6">
                    <h6 class="principal-texto">DATOS DEL USUARIO QUE LLEVA LA CONSULTA</h6>
                    <div class="box box-success">
                        <div class="col-lg-12">
                            @Html.LabelFor(model => model.CedulaUsuario)
                            @Html.EditorFor(model => model.CedulaUsuario, new { htmlAttributes = new { @class = "form-control", @disabled = "disabled" } })
                        </div>
                        <div class="col-lg-12">
                            @Html.LabelFor(model => model.Nombre)
                            @Html.EditorFor(model => model.Nombre, new { htmlAttributes = new { @class = "form-control", @disabled = "disabled" } })
                        </div>
                        <div class="col-lg-12">
                            @Html.LabelFor(model => model.Apellido1)
                            @Html.EditorFor(model => model.Apellido1, new { htmlAttributes = new { @class = "form-control", @disabled = "disabled" } })
                        </div>
                        <div class="col-lg-12">
                            @Html.LabelFor(model => model.Apellido2)
                            @Html.EditorFor(model => model.Apellido2, new { htmlAttributes = new { @class = "form-control", @disabled = "disabled" } })
                        </div>
                    </div>
                </div>
            </div>
            <div class="col-lg-12">
                <h6 class="principal-texto">DATOS DE LA SOLICITUD</h6>
                <div class="box box-warning">
                    <div class="col-lg-6">
                        <div class="form-group">
                            <div class="col-lg-12">
                                @Html.LabelFor(model => model.CodigoPrestamoEquipo)
                                @Html.EditorFor(model => model.CodigoPrestamoEquipo, new { htmlAttributes = new { @class = "form-control", @disabled = "disabled" } })
                            </div>
                            <div class="form-group">
                                <div class="col-lg-12">
                                    @Html.LabelFor(model => model.Departamento)
                                    @Html.EditorFor(model => model.Departamento, new { htmlAttributes = new { @class = "form-control", @disabled = "disabled" } })
                                </div>
                            </div>
                            <div class="form-group">
                                <div class="col-lg-12">
                                    @Html.LabelFor(model => model.TipoEquipo)
                                    @Html.EditorFor(model => model.TipoEquipo, new { htmlAttributes = new { @class = "form-control", @disabled = "disabled" } })
                                </div>
                            </div>
                            <div class="form-group">
                                <div class="col-lg-12">
                                    @Html.LabelFor(model => model.Implementos)
                                    @Html.EditorFor(model => model.Implementos, new { htmlAttributes = new { @class = "form-control", @disabled = "disabled" } })
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="col-lg-6">
                        <div class="form-group">
                            <div class="col-lg-12">
                                @Html.LabelFor(model => model.EspecificacionImplementos)
                                @Html.EditorFor(model => model.EspecificacionImplementos, new { htmlAttributes = new { @class = "form-control", @disabled = "disabled" } })
                            </div>
                        </div>
                        <div class="form-group">
                            <div class="col-lg-12">
                                @Html.LabelFor(model => model.FechaIngreso)
                                @Html.EditorFor(model => model.FechaIngreso, new { htmlAttributes = new { @class = "form-control", @disabled = "disabled" } })
                            </div>
                        </div>
                        <div class="form-group">
                            <div class="col-lg-12">
                                @Html.LabelFor(model => model.FechaRespuesta)
