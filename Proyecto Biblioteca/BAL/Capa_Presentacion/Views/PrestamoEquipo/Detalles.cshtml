                            </div>
                        </div>
                        <div class="form-group">
                            <div class="col-lg-12">
                                @Html.LabelFor(model => model.Estado)
                                @Html.EditorFor(model => model.Estado, new { htmlAttributes = new { @class = "form-control", @disabled = "disabled" } })
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <div class="col-lg-12">
                <br />
                <div class="principal-texto">
                    @Html.ActionLink("Regresar", "Index", "PrestamoEquipo", null, new { @class = "btn btn-danger" })
                </div>
            </div>
        </div>
    </div>
</div>
