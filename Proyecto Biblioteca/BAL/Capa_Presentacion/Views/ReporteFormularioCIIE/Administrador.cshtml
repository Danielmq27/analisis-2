@model IEnumerable<Capa_Presentacion.Models.ReporteFormularioCIIE>

@{
    ViewBag.Title = "CantidadTipoUsuario";
    Layout = "~/Views/Shared/_Administrador.cshtml";
}
<div class="content">
    @Html.Raw(TempData["msg"])
    <h4 class="principal-texto">Reportes de cantidad de usuarios por rangos de fecha</h4>
    <div class="col-lg-4">
        <form action="@Url.Action("CantidadUsuariosFechaIngreso", "ReporteFormularioCIIE")" id="frm" method="post">
            <div class="form-group">
                <div class="col-sm-12">
                    <h4 class="titulo-reporte">Reporte fechas de ingreso</h4>
                </div>
            </div>
            <div class="form-group has-feedback col-lg-12">
                <label>Fecha Ingreso</label>
                <input type="date" class="form-control" id="Fecha1" name="Fecha1" autocomplete="off" required>
            </div>
            <div class="form-group has-feedback col-lg-12">
                <label>Fecha Ingreso</label>
                <input type="date" class="form-control" id="Fecha2" name="Fecha2" autocomplete="off" required>
            </div>
            <div class="form-group">
                <div class="col-xs-4">
                    <button type="submit" class="btn btn-success btn-block btn-flat">Generar</button>
                </div>
            </div>
        </form>
    </div>
    <div class="col-lg-4">
        <form action="@Url.Action("CantidadUsuariosFechaRespuesta", "ReporteFormularioCIIE")" id="frm" method="post">
            <div class="form-group">
                <div class="col-xs-12">
                    <h4 class="titulo-reporte">Reporte por fechas de respuesta</h4>
                </div>
            </div>
            <div class="form-group has-feedback col-lg-12">
                <label>Fecha Respuesta</label>
                <input type="date" class="form-control" id="Fecha1" name="Fecha1" autocomplete="off" required>
            </div>
            <div class="form-group has-feedback col-lg-12">
                <label>Fecha Respuesta</label>
                <input type="date" class="form-control" id="Fecha2" name="Fecha2" autocomplete="off" required>
            </div>
            <div class="form-group">
                <div class="col-xs-4">
                    <button type="submit" class="btn btn-success btn-block btn-flat">Generar</button>
                </div>
            </div>
        </form>
    </div>
    <div class="col-lg-4">
        <form action="@Url.Action("CantidadUsuariosFechaRespuesta", "ReporteFormularioCIIE")" id="frm" method="post">
            <div class="form-group">
                <div class="col-lg-12">
                    <h4 class="titulo-reporte">Reporte por fechas de ingreso y respuesta</h4>
                </div>
            </div>
            <div class="form-group has-feedback col-lg-12">
                <label>Fecha Ingreso</label>
                <input type="date" class="form-control" id="Fecha1" name="Fecha1" autocomplete="off" required>
            </div>
            <div class="form-group has-feedback col-lg-12">
                <label>Fecha Respuesta</label>
                <input type="date" class="form-control" id="Fecha2" name="Fecha2" autocomplete="off" required>
            </div>
            <div class="form-group">
                <div class="col-xs-4">
                    <button type="submit" class="btn btn-success btn-block btn-flat">Generar</button>
                </div>
            </div>
        </form>
    </div>
</div>

