@model IEnumerable<Capa_Presentacion.Models.ReporteFormularioCIIE>

@{
    ViewBag.Title = "CantidadTipoUsuario";
    Layout = "~/Views/Shared/_Editar.cshtml";
}
<div class="content">
    @Html.Raw(TempData["msg"])
    <form action="@Url.Action("ReporteCantidadTipoUsuario", "Reporte")" id="frm" method="post">
        <div class="form-group">
            <div class="col-xs-12">
                <h4>Reporte de cantidad de usuarios por fecha de ingreso</h4>
            </div>
        </div>
        <div class="form-group has-feedback col-lg-5">
            <input type="date" class="form-control" placeholder="Correo" id="fechaFrom" name="fechaFrom" autocomplete="off" required>
        </div>
        <div class="form-group has-feedback col-lg-5">
            <input type="date" class="form-control" placeholder="Contraseña" id="fechaTo" name="fechaTo" autocomplete="off" required>
        </div>
        <div class="form-group">
            <div class="col-xs-4">
                <button type="submit" class="btn btn-success btn-block btn-flat">Generar Reporte</button>
            </div>
        </div>
    </form>
</div>

